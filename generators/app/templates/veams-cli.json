{
	"projectType": "static-page-app",
	"blueprints": {
		"container": {
			"skipImports": true,
			"prompts": "node_modules/veams-bp-react-container/prompts",
			"templates": "node_modules/veams-bp-react-container/templates"
		},
		"store": {
			"skipImports": true,
			"prompts": "node_modules/veams-bp-redux/prompts",
			"templates": "node_modules/veams-bp-redux/templates"
		}
	},
	"config": {
		"src": "configs/tasks/_grunt/*.js"
	},
	"importFiles": {
		"style": "src/app/app.scss",
		"script": "src/app/app.js"
	},
	"insertpoints": [
		"src/app/app.js",
		"src/app/core/layouts",
		"src/app/pages",
		"src/app/features",
		"src/app/app.events.js"
	],
	"paths": {
		"app": "app",
		"assets": "src/app/assets",
		"component": "src/app/shared/components",
		"config": "configs",
		"server": "src/server",
		"src": "src/app",
		"utility": "src/app/shared/utilities"
	},
	"ports": {
		"app": 3000,
		"server": 2999
	},
	"startPath": "index.html"
}